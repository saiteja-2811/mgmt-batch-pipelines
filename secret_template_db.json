{
   "apiVersion": "v1",
   "kind": "Secret",
   "metadata": {
      "name": "dbaccess"
   },
   "type": "Opaque",
   "stringData": {
      "host": "REPLACE_PG_HOST",
      "password": "REPLACE_PG_PASSWORD",
      "user": "REPLACE_PG_USER",
      "dbname": "REPLACE_PG_DBNAME",
      "sslrootcert": "REPLACE_PG_SSLROOTCERT",
      "sslcert": "REPLACE_PG_SSLCERT",
      "sslkey": "REPLACE_PG_SSLKEY"
   }
}
